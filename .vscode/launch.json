{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": "Launch psychoshare_api",
      "program": "${workspaceFolder}/psychoshare_api/bin/Debug/net8.0/psychoshare_api.dll",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}/psychoshare_api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
